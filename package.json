{
  "name": "boilerplate-vanilla",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Gerson Lima <gersonthiago.lima@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --config config/webpack.config.js",
    "build": "NODE_ENV=production webpack --mode production --config config/webpack.config.js",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "src/styles/**/*.scss": [
      "stylelint --syntax=scss",
      "git add"
    ],
    "src/javascripts/**/*.js": [
      "eslint",
      "git add"
    ]
  },
  "pre-commit": "lint-staged",
  "browserslist": {
    "production": [
      "> 1% in AU",
      "> 1% in JP",
      "last 2 Chrome versions",
      "last 2 Firefox versions",
      "last 2 ChromeAndroid versions",
      "last 2 Edge versions",
      "last 2 iOS versions",
      "last 2 Safari versions",
      "not ie <= 9"
    ],
    "development": [
      "last 1 Chrome version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "autoprefixer": "^9.6.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.13.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "html-withimg-loader": "^0.1.16",
    "imagemin-webpack-plugin": "^2.4.2",
    "lint-staged": "^8.2.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.11.0",
    "nunjucks": "^3.2.0",
    "nunjucks-webpack-loader": "^1.0.8",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.2.1",
    "svg-url-loader": "^2.3.2",
    "terser-webpack-plugin": "^2.1.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-livereload-plugin": "^2.2.0"
  },
  "dependencies": {
    "promise-polyfill": "^8.1.3",
    "smoothscroll-polyfill": "^0.4.4",
    "whatwg-fetch": "^3.0.0"
  }
}
